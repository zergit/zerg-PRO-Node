generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model UserModel {
  id        Int      @id @default(autoincrement())
  name      String
  email     String
  password  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model ProductsModel {
  id          Int      @id @default(autoincrement())
  title       String
  price       Int
  appointmen  String
  description String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}

model LocationModel {
  id      Int    @id @default(autoincrement())
  city    String
  address String
}
